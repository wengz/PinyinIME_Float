<keyboard
  skb_template="@xml/skb_template1"
  skb_cache_flag="true"
  key_xmargin="0.607%p"
  key_ymargin="1.960%p"
  width="9.311%p"
  height="23.529%p"
  key_type="0"
  repeat="false"
  balloon="true">

  <row start_pos_x="3.441%p" start_pos_y="2.941%p">
    <keys splitter="|" labels="~|±|×|÷|•|°|&lt;|&gt;|{|}"/>
  </row>

  <row start_pos_x="3.441%p" start_pos_y="26.470%p">
    <keys splitter="|" labels="©|£|€|^|®"/>
    <key label="℃">
      <toggle_state state_id="@string/toggle_en_sym2" label="℉"/>
    </key>
    <keys splitter="|" labels="_|="/>
    <key label="【">
      <toggle_state state_id="@string/toggle_en_sym2" label="["/>
    </key>
    <key label="】">
      <toggle_state state_id="@string/toggle_en_sym2" label="]"/>
    </key>
  </row>

  <row start_pos_x="3.441%p" start_pos_y="50%p">
    <key code="-5" width="12.55%p" key_type="1" label="ALT"/>
    <key label="™"/>
    <key label="‘">
      <toggle_state state_id="@string/toggle_en_sym2" label="‘"/>
    </key>
    <key label="’">
      <toggle_state state_id="@string/toggle_en_sym2" label="’"/>
    </key>
    <keys splitter=" " labels="+ | \\ √"/>
    <key start_pos_x="84.008%p" width="12.55%p" code="67" key_type="1" repeat="true"/>
  </row>

  <row start_pos_x="3.441%p" start_pos_y="73.529%p">
    <key code="-2" label="中 符" width="12.55%p" repeat="true" key_type="1">
      <toggle_state state_id="@string/toggle_en_sym2" label="英 符" code="-2"/>
    </key>
    <key code="-3" key_type="1" width="12.55%p" label="\?123"/>
    <key code="62" key_type="0" width="38.056%p" label="Space"/>
    <key label="……" key_type="0" width="12.55%p">
      <toggle_state state_id="@string/toggle_en_sym2" label="…"/>
    </key>
    <key width="17.408%p"  code="66" key_type="1">
      <toggle_state state_id="@string/toggle_enter_go" label="去往" code="66"/>
      <toggle_state state_id="@string/toggle_enter_search" code="66"
          icon="@drawable/search_icon" icon_popup="@drawable/search_popup_icon"/>
      <toggle_state state_id="@string/toggle_enter_send" label="发送" code="66"/>
      <toggle_state state_id="@string/toggle_enter_next" label="下一个" code="66"/>
      <toggle_state state_id="@string/toggle_enter_done" label="完成" code="66"/>
    </key>
  </row>
</keyboard>

